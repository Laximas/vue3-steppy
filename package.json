{
  "name": "vue3-steppy",
  "description": "A customizable Vue 3 Stepper plugin",
  "version": "1.4.1",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "author": "Mike Konstantakos",
  "homepage": "https://laximas.github.io/vue3-steppy/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laximas/vue3-steppy.git"
  },
  "bugs": {
    "url": "https://github.com/Laximas/vue3-steppy/issues"
  },
  "keywords": [
    "vue3",
    "vue",
    "stepper",
    "steppy",
    "vue-stepper",
    "vue-steppy",
    "vue3-stepper",
    "vue3-steppy"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-steppy.umd.cjs",
  "module": "./dist/vue3-steppy.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-steppy.js",
      "require": "./dist/vue3-steppy.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && shx cp ./src/index.d.ts dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.72.0",
    "shx": "^0.3.4",
    "typescript": "^5.4.3",
    "vite": "^4.5.2",
    "vite-plugin-css-injected-by-js": "^2.4.0"
  },
  "directories": {
    "lib": "lib"
  }
}
